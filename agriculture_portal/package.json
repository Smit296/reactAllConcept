{
  "name": "agriculture_portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "client": "cd client && npm start",
    "server": "nodemon ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.6.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.9",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "react-alert": "^7.0.0",
    "react-alert-template-basic": "^1.0.0",
    "sequelize": "^5.22.4",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "engines": {
    "node": "10.16.x",
    "npm": "6.14.x"
  }
}
